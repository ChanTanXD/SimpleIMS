<Window x:Class="INV_MGMT_SYS.InsertWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:INV_MGMT_SYS"
        mc:Ignorable="d"
        Title="InsertWin" Height="450" Width="800">
    <Grid>
        <Button Content="Return" HorizontalAlignment="Left" Margin="28,22,0,0" VerticalAlignment="Top"
                Name="ReturnBtn" Click="ReturnOnClick"/>
        <TextBlock HorizontalAlignment="Left" Margin="339,80,0,0" TextWrapping="Wrap" Text="Brand" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="378,79,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <TextBlock HorizontalAlignment="Left" Margin="339,112,0,0" TextWrapping="Wrap" Text="HP" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="378,111,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <TextBlock HorizontalAlignment="Left" Margin="339,151,0,0" TextWrapping="Wrap" Text="Model" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="378,150,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <TextBlock HorizontalAlignment="Left" Margin="339,188,0,0" TextWrapping="Wrap" Text="Series" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="378,187,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <TextBlock HorizontalAlignment="Left" Margin="339,229,0,0" TextWrapping="Wrap" Text="Price" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="378,228,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <TextBlock HorizontalAlignment="Left" Margin="339,264,0,0" TextWrapping="Wrap" Text="Stock" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="378,263,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

        <TextBlock HorizontalAlignment="Left" Margin="339,302,0,0" TextWrapping="Wrap" Text="Catalogue link" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Center" Margin="0,323,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        
        <Button Content="Insert new item" HorizontalAlignment="Left" Margin="378,383,0,0" VerticalAlignment="Top"
                Name="InsertBtn" Click="InsertOnClick"/>

    </Grid>
</Window>
